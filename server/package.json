    "@types/express-session": "^1.17.6",
    "@types/mssql": "^8.1.2",
    "@types/node": "^18.14.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "nodemon": "^2.0.21",
    "rimraf": "^4.3.1",
