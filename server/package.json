{
  "name": "mikane_server",
  "version": "2.0.0",
  "description": "API server for Mikane",
  "scripts": {
    "build": "rimraf ./build && tsc --project ./",
    "start": "node build/index.js",
    "startts": "npx ts-node src/index.ts",
    "dev": "npx nodemon src/index.ts",
    "test": "vitest run --threads=false --reporter=verbose --coverage",
    "db": "docker-compose -f test_db/docker-compose-test-db.yml -p mikane_test_db up -d"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "nodemailer": "^6.9.3",
    "pg": "^8.11.1",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^18.16.19",
    "@types/nodemailer": "^6.4.8",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.44.0",
    "nodemailer-mock": "^2.0.1",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.32.4"
  },
  "license": "GPL-3.0-or-later"
}
