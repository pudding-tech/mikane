{
  "name": "mikane_server",
  "version": "2.0.0",
  "description": "API server for Mikane",
  "scripts": {
    "build": "rimraf ./build && tsc --project ./",
    "start": "npx ts-node src/server.ts",
    "startbuild": "node build/server.js",
    "dev": "npx nodemon src/server.ts",
    "dev2": "concurrently \"tsc -w\" \"npx nodemon build/server.js\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "mssql": "^9.1.1",
    "nodemailer": "^6.9.2",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/mssql": "^8.1.2",
    "@types/node": "^18.16.13",
    "@types/nodemailer": "^6.4.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "license": "GPL-3.0-or-later"
}
